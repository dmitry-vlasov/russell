$)
${
	$v ph ps $.
	f0 $f wff ph $.
	f1 $f wff ps $.
	ppm2.54 $p |- ( ( -. ph -> ps ) -> ( ph \/ ps ) ) $=
	f0 f1 adf-or f0 f1 awo f0 awn f1 awi pbiimpri $.
$}
