     (New usage is discouraged.) */

theorem merlem4 (ph : wff, th : wff, ta : wff)  {
	prop 1 : wff = |- ( ta → ( ( ta → ph ) → ( th → ph ) ) ) ;;
}

proof of merlem4 {
	step 1 : wff = ax-meredith () |- ( ( ( ( ( ph → ph ) → ( ¬ th → ¬ th ) ) → th ) → ta ) → ( ( ta → ph ) → ( th → ph ) ) ) ;;
	step 2 : wff = merlem3 () |- ( ( ( ( ( ( ph → ph ) → ( ¬ th → ¬ th ) ) → th ) → ta ) → ( ( ta → ph ) → ( th → ph ) ) ) → ( ta → ( ( ta → ph ) → ( th → ph ) ) ) ) ;;
	step 3 : wff = ax-mp (step 1, step 2) |- ( ta → ( ( ta → ph ) → ( th → ph ) ) ) ;;
	qed prop 1 = step 3 ;;
}

/* Step 11 of Meredith's proof of Lukasiewicz axioms from his sole axiom.
     (Contributed by NM, 14-Dec-2002.)  (Proof modification is discouraged.)
     (New usage is discouraged.) */

theorem merlem5 (ph : wff, ps : wff)  {
	prop 1 : wff = |- ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ;;
}

proof of merlem5 {
	step 1 : wff = ax-meredith () |- ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ;;
	step 2 : wff = ax-meredith () |- ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) ;;
	step 3 : wff = merlem1 () |- ( ( ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) → ( ¬ ph → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) ) ;;
	step 4 : wff = merlem4 () |- ( ( ( ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) → ( ¬ ph → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) ) → ( ( ( ( ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) → ( ¬ ph → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) ) → ph ) → ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) ) ) ;;
	step 5 : wff = ax-mp (step 3, step 4) |- ( ( ( ( ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) → ( ¬ ph → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) ) → ph ) → ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) ) ;;
	step 6 : wff = ax-meredith () |- ( ( ( ( ( ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) → ( ¬ ph → ¬ ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) ) ) → ph ) → ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) ) → ( ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) → ( ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) ) ) ;;
	step 7 : wff = ax-mp (step 5, step 6) |- ( ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ¬ ¬ ph ) ) → ps ) → ph ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) → ( ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) ) ;;
	step 8 : wff = ax-mp (step 2, step 7) |- ( ( ( ( ( ( ps → ps ) → ( ¬ ps → ¬ ps ) ) → ps ) → ps ) → ( ( ps → ps ) → ( ps → ps ) ) ) → ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ) ;;
	step 9 : wff = ax-mp (step 1, step 8) |- ( ( ph → ps ) → ( ¬ ¬ ph → ps ) ) ;;
	qed prop 1 = step 9 ;;
