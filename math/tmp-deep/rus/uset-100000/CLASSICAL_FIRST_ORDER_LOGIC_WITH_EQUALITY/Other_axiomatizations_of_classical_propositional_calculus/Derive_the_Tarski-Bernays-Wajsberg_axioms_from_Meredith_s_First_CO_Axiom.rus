	step 3 : wff = merco1lem2 () |- ( ( ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) → ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) ) → ( ( ( ( ph → ph ) → F. ) → ( ( ( ph → ph ) → ( ph → F. ) ) → F. ) ) → ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) ) ) ;;
	step 4 : wff = ax-mp (step 2, step 3) |- ( ( ( ( ph → ph ) → F. ) → ( ( ( ph → ph ) → ( ph → F. ) ) → F. ) ) → ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) ) ;;
	step 5 : wff = ax-mp (step 1, step 4) |- ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) ;;
	step 6 : wff = merco1lem2 () |- ( ( ( ch → ph ) → F. ) → ( ( ( ph → ps ) → ( ch → F. ) ) → F. ) ) ;;
	step 7 : wff = retbwax2 () |- ( ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) → ( ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) → ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ) ) ;;
	step 8 : wff = merco1lem2 () |- ( ( ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) → ( ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) → ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ) ) → ( ( ( ( ch → ph ) → F. ) → ( ( ( ph → ps ) → ( ch → F. ) ) → F. ) ) → ( ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) → ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ) ) ) ;;
	step 9 : wff = ax-mp (step 7, step 8) |- ( ( ( ( ch → ph ) → F. ) → ( ( ( ph → ps ) → ( ch → F. ) ) → F. ) ) → ( ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) → ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ) ) ;;
	step 10 : wff = ax-mp (step 6, step 9) |- ( ( ph → ( ( ( ph → ph ) → ( ph → F. ) ) → ( ph → ph ) ) ) → ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ) ;;
	step 11 : wff = ax-mp (step 5, step 10) |- ( ( ( ph → ps ) → ( ch → F. ) ) → ( ch → ph ) ) ;;
	qed prop 1 = step 11 ;;
}

