theorem merco1lem1 (ph : wff, ch : wff)  {
	prop 1 : wff = |- ( ph → ( F. → ch ) ) ;;
}

proof of merco1lem1 {
	step 1 : wff = merco1 () |- ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ( ( F. → ph ) → F. ) → ( ph → F. ) ) ) ;;
	step 2 : wff = merco1 () |- ( ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ( ( F. → ph ) → F. ) → ( ph → F. ) ) ) → ( ( ( ( ( F. → ph ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ) ;;
	step 3 : wff = ax-mp (step 1, step 2) |- ( ( ( ( ( F. → ph ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ;;
	step 4 : wff = merco1 () |- ( ( ( ( ( ( F. → ph ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) → ( ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ) ;;
	step 5 : wff = ax-mp (step 3, step 4) |- ( ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ;;
	step 6 : wff = merco1 () |- ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ( ph → ( F. → ph ) ) → F. ) ) → ( ph → ( F. → ph ) ) ) → ( ( ( ph → ( F. → ph ) ) → F. ) → ( ph → F. ) ) ) ;;
	step 7 : wff = merco1 () |- ( ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ( ph → ( F. → ph ) ) → F. ) ) → ( ph → ( F. → ph ) ) ) → ( ( ( ph → ( F. → ph ) ) → F. ) → ( ph → F. ) ) ) → ( ( ( ( ( ph → ( F. → ph ) ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) ) ) ;;
	step 8 : wff = ax-mp (step 6, step 7) |- ( ( ( ( ( ph → ( F. → ph ) ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) ) ;;
	step 9 : wff = merco1 () |- ( ( ( ( ( ( ph → ( F. → ph ) ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) ) → ( ( ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) → ( ph → ( ph → ( F. → ph ) ) ) ) ) ;;
	step 10 : wff = ax-mp (step 8, step 9) |- ( ( ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) → ( ph → ( ph → ( F. → ph ) ) ) ) ;;
	step 11 : wff = ax-mp (step 5, step 10) |- ( ph → ( ph → ( F. → ph ) ) ) ;;
	step 12 : wff = merco1 () |- ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ph → F. ) ) → ( F. → ch ) ) → ( ( ( F. → ch ) → F. ) → ( ph → F. ) ) ) ;;
	step 13 : wff = merco1 () |- ( ( ( ( ( ( F. → ph ) → ( ph → F. ) ) → ( ph → F. ) ) → ( F. → ch ) ) → ( ( ( F. → ch ) → F. ) → ( ph → F. ) ) ) → ( ( ( ( ( F. → ch ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ) ;;
	step 14 : wff = ax-mp (step 12, step 13) |- ( ( ( ( ( F. → ch ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) ;;
	step 15 : wff = merco1 () |- ( ( ( ( ( ( F. → ch ) → F. ) → ( ph → F. ) ) → ( F. → ph ) ) → ( ph → ( F. → ph ) ) ) → ( ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) → ( ph → ( F. → ch ) ) ) ) ;;
	step 16 : wff = ax-mp (step 14, step 15) |- ( ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) → ( ph → ( F. → ch ) ) ) ;;
	step 17 : wff = merco1 () |- ( ( ( ( ( F. → ch ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) → ( ( ph → ( F. → ph ) ) → F. ) ) → ( ph → ( F. → ch ) ) ) → ( ( ( ph → ( F. → ch ) ) → F. ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) ) ;;
	step 18 : wff = merco1 () |- ( ( ( ( ( ( F. → ch ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) → ( ( ph → ( F. → ph ) ) → F. ) ) → ( ph → ( F. → ch ) ) ) → ( ( ( ph → ( F. → ch ) ) → F. ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) ) → ( ( ( ( ( ph → ( F. → ch ) ) → F. ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) → ( F. → ch ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) ) ) ;;
	step 19 : wff = ax-mp (step 17, step 18) |- ( ( ( ( ( ph → ( F. → ch ) ) → F. ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) → ( F. → ch ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) ) ;;
	step 20 : wff = merco1 () |- ( ( ( ( ( ( ph → ( F. → ch ) ) → F. ) → ( ( ph → ( ph → ( F. → ph ) ) ) → F. ) ) → ( F. → ch ) ) → ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) ) → ( ( ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) → ( ph → ( F. → ch ) ) ) → ( ( ph → ( ph → ( F. → ph ) ) ) → ( ph → ( F. → ch ) ) ) ) ) ;;
	step 21 : wff = ax-mp (step 19, step 20) |- ( ( ( ( ph → ( F. → ph ) ) → ( F. → ch ) ) → ( ph → ( F. → ch ) ) ) → ( ( ph → ( ph → ( F. → ph ) ) ) → ( ph → ( F. → ch ) ) ) ) ;;
	step 22 : wff = ax-mp (step 16, step 21) |- ( ( ph → ( ph → ( F. → ph ) ) ) → ( ph → ( F. → ch ) ) ) ;;
	step 23 : wff = ax-mp (step 11, step 22) |- ( ph → ( F. → ch ) ) ;;
	qed prop 1 = step 23 ;;
}

/*~ tbw-ax4 rederived from ~ merco1 .  (Contributed by Anthony Hart,
     17-Sep-2011.)  (Proof modification is discouraged.)
     (New usage is discouraged.) */

theorem retbwax4 (ph : wff)  {
	prop 1 : wff = |- ( F. → ph ) ;;
}

proof of retbwax4 {
	step 1 : wff = merco1lem1 () |- ( ph → ( F. → ph ) ) ;;
	step 2 : wff = merco1lem1 () |- ( ( ph → ( F. → ph ) ) → ( F. → ph ) ) ;;
	step 3 : wff = ax-mp (step 1, step 2) |- ( F. → ph ) ;;
	qed prop 1 = step 3 ;;
}

