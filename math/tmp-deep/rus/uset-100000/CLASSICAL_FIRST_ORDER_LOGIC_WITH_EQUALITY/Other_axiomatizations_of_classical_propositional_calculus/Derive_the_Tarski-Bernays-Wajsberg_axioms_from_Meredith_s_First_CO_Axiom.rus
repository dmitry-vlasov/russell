theorem merco1lem2 (ph : wff, ps : wff, ch : wff, ta : wff)  {
	prop 1 : wff = |- ( ( ( ph → ps ) → ch ) → ( ( ( ps → ta ) → ( ph → F. ) ) → ch ) ) ;;
}

proof of merco1lem2 {
	step 1 : wff = retbwax2 () |- ( ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) → ( ( ch → ph ) → ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) ) ) ;;
	step 2 : wff = merco1 () |- ( ( ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) → ( ( ch → ph ) → ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) ) ) → ( ( ( ( ch → ph ) → ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) ) → ps ) → ( ph → ps ) ) ) ;;
	step 3 : wff = ax-mp (step 1, step 2) |- ( ( ( ( ch → ph ) → ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) ) → ps ) → ( ph → ps ) ) ;;
	step 4 : wff = merco1 () |- ( ( ( ( ( ch → ph ) → ( ( ( ps → ta ) → ( ph → F. ) ) → F. ) ) → ps ) → ( ph → ps ) ) → ( ( ( ph → ps ) → ch ) → ( ( ( ps → ta ) → ( ph → F. ) ) → ch ) ) ) ;;
	step 5 : wff = ax-mp (step 3, step 4) |- ( ( ( ph → ps ) → ch ) → ( ( ( ps → ta ) → ( ph → F. ) ) → ch ) ) ;;
	qed prop 1 = step 5 ;;
}

