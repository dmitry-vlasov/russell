proof of nebi {
	step 1 : wff = id () |- ( ( A = B ↔ C = D ) → ( A = B ↔ C = D ) ) ;;
	step 2 : wff = necon3bid (step 1) |- ( ( A = B ↔ C = D ) → ( A ≠ B ↔ C ≠ D ) ) ;;
	step 3 : wff = id () |- ( ( A ≠ B ↔ C ≠ D ) → ( A ≠ B ↔ C ≠ D ) ) ;;
	step 4 : wff = necon4bid (step 3) |- ( ( A ≠ B ↔ C ≠ D ) → ( A = B ↔ C = D ) ) ;;
	step 5 : wff = impbii (step 2, step 4) |- ( ( A = B ↔ C = D ) ↔ ( A ≠ B ↔ C ≠ D ) ) ;;
