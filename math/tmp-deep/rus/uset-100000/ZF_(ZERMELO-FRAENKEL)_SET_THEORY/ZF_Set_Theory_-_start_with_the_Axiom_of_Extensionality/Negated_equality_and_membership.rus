	step 3 : wff = bitri (step 1, step 2) |- ( A ≠ C ↔ B ≠ D ) ;;
	qed prop 1 = step 3 ;;
}

/*Deduction for inequality.  (Contributed by NM, 25-Oct-1999.) */

theorem neeq1d (ph : wff, A : class, B : class, C : class)  {
