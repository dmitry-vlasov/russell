proof of necom {
	step 1 : wff = eqcom () |- ( A = B ↔ B = A ) ;;
	step 2 : wff = necon3bii (step 1) |- ( A ≠ B ↔ B ≠ A ) ;;
	qed prop 1 = step 2 ;;
}

