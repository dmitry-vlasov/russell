}

/*Theorem *13.18 in [WhiteheadRussell] p. 178.  (Contributed by Andrew
     Salmon, 3-Jun-2011.) */

theorem pm13.18 (A : class, B : class, C : class)  {
	prop 1 : wff = |- ( ( A = B ∧ A ≠ C ) → B ≠ C ) ;;
}

proof of pm13.18 {
	step 1 : wff = eqeq1 () |- ( A = B → ( A = C ↔ B = C ) ) ;;
	step 2 : wff = biimprd (step 1) |- ( A = B → ( B = C → A = C ) ) ;;
	step 3 : wff = necon3d (step 2) |- ( A = B → ( A ≠ C → B ≠ C ) ) ;;
	step 4 : wff = imp (step 3) |- ( ( A = B ∧ A ≠ C ) → B ≠ C ) ;;
	qed prop 1 = step 4 ;;
}

