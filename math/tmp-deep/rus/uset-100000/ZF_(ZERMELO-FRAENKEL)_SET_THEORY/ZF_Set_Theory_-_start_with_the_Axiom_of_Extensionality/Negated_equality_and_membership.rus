	step 4 : wff = bitr4i (step 1, step 3) |- ( ( A = B ∨ ps ) ↔ ( A ≠ B → ps ) ) ;;
	qed prop 1 = step 4 ;;
}

/*A De Morgan's law for inequality.  (Contributed by NM, 18-May-2007.) */

theorem neanior (A : class, B : class, C : class, D : class)  {
	prop 1 : wff = |- ( ( A ≠ B ∧ C ≠ D ) ↔ ¬ ( A = B ∨ C = D ) ) ;;
