/*=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
        Negated equality and membership

=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
*/

/*Declare new connectives. */

constant {
	symbol ≠ ;;
	ascii =/= ;;
	latex \ne ;;
}

/*Not equal to (equal sign with slash through it). */

constant {
	symbol ∉ ;;
	ascii e/ ;;
	latex \notin ;;
}

/*Not an element of (epsilon with slash through it). */

/*Extend wff notation to include inequality. */

rule wne (A : class, B : class) {
	term : wff = # A ≠ B ;;
}

/*Extend wff notation to include negated membership. */

rule wnel (A : class, B : class) {
	term : wff = # A ∉ B ;;
}

/*Define inequality.  (Contributed by NM, 5-Aug-1993.) */

definition df-ne (A : class, B : class)  {
	defiendum : wff = # A ≠ B ;;
	definiens : wff = # ¬ A = B ;;
	-----------------------
	prop : wff = |- ( defiendum ↔ definiens ) ;;
}

/*Define negated membership.  (Contributed by NM, 7-Aug-1994.) */

definition df-nel (A : class, B : class)  {
	defiendum : wff = # A ∉ B ;;
	definiens : wff = # ¬ A ∈ B ;;
	-----------------------
	prop : wff = |- ( defiendum ↔ definiens ) ;;
}

/*Negation of inequality.  (Contributed by NM, 9-Jun-2006.) */

theorem nne (A : class, B : class)  {
	prop 1 : wff = |- ( ¬ A ≠ B ↔ A = B ) ;;
}

proof of nne {
	step 1 : wff = df-ne () |- ( A ≠ B ↔ ¬ A = B ) ;;
	step 2 : wff = con2bii (step 1) |- ( A = B ↔ ¬ A ≠ B ) ;;
	step 3 : wff = bicomi (step 2) |- ( ¬ A ≠ B ↔ A = B ) ;;
	qed prop 1 = step 3 ;;
}

/*No class is unequal to itself.  (Contributed by Stefan O'Rear,
     1-Jan-2015.) */

theorem neirr (A : class)  {
	prop 1 : wff = |- ¬ A ≠ A ;;
}

proof of neirr {
	step 1 : wff = eqid () |- A = A ;;
	step 2 : wff = nne () |- ( ¬ A ≠ A ↔ A = A ) ;;
	step 3 : wff = mpbir (step 1, step 2) |- ¬ A ≠ A ;;
	qed prop 1 = step 3 ;;
}

/*Excluded middle with equality and inequality.  (Contributed by NM,
     3-Feb-2012.) */

theorem exmidne (A : class, B : class)  {
	prop 1 : wff = |- ( A = B ∨ A ≠ B ) ;;
}

proof of exmidne {
	step 1 : wff = exmid () |- ( A = B ∨ ¬ A = B ) ;;
	step 2 : wff = df-ne () |- ( A ≠ B ↔ ¬ A = B ) ;;
	step 3 : wff = orbi2i (step 2) |- ( ( A = B ∨ A ≠ B ) ↔ ( A = B ∨ ¬ A = B ) ) ;;
	step 4 : wff = mpbir (step 1, step 3) |- ( A = B ∨ A ≠ B ) ;;
	qed prop 1 = step 4 ;;
}

/*Law of noncontradiction with equality and inequality.  (Contributed by NM,
     3-Feb-2012.) */

theorem nonconne (A : class, B : class)  {
	prop 1 : wff = |- ¬ ( A = B ∧ A ≠ B ) ;;
}

proof of nonconne {
	step 1 : wff = pm3.24 () |- ¬ ( A = B ∧ ¬ A = B ) ;;
	step 2 : wff = df-ne () |- ( A ≠ B ↔ ¬ A = B ) ;;
	step 3 : wff = anbi2i (step 2) |- ( ( A = B ∧ A ≠ B ) ↔ ( A = B ∧ ¬ A = B ) ) ;;
	step 4 : wff = mtbir (step 1, step 3) |- ¬ ( A = B ∧ A ≠ B ) ;;
	qed prop 1 = step 4 ;;
}

/*Equality theorem for inequality.  (Contributed by NM, 19-Nov-1994.) */

theorem neeq1 (A : class, B : class, C : class)  {
	prop 1 : wff = |- ( A = B → ( A ≠ C ↔ B ≠ C ) ) ;;
}

proof of neeq1 {
	step 1 : wff = eqeq1 () |- ( A = B → ( A = C ↔ B = C ) ) ;;
	step 2 : wff = notbid (step 1) |- ( A = B → ( ¬ A = C ↔ ¬ B = C ) ) ;;
	step 3 : wff = df-ne () |- ( A ≠ C ↔ ¬ A = C ) ;;
	step 4 : wff = df-ne () |- ( B ≠ C ↔ ¬ B = C ) ;;
	step 5 : wff = 3bitr4g (step 2, step 3, step 4) |- ( A = B → ( A ≠ C ↔ B ≠ C ) ) ;;
	qed prop 1 = step 5 ;;
}

/*Equality theorem for inequality.  (Contributed by NM, 19-Nov-1994.) */

theorem neeq2 (A : class, B : class, C : class)  {
	prop 1 : wff = |- ( A = B → ( C ≠ A ↔ C ≠ B ) ) ;;
}

proof of neeq2 {
	step 1 : wff = eqeq2 () |- ( A = B → ( C = A ↔ C = B ) ) ;;
	step 2 : wff = notbid (step 1) |- ( A = B → ( ¬ C = A ↔ ¬ C = B ) ) ;;
	step 3 : wff = df-ne () |- ( C ≠ A ↔ ¬ C = A ) ;;
	step 4 : wff = df-ne () |- ( C ≠ B ↔ ¬ C = B ) ;;
	step 5 : wff = 3bitr4g (step 2, step 3, step 4) |- ( A = B → ( C ≠ A ↔ C ≠ B ) ) ;;
	qed prop 1 = step 5 ;;
}

/*Inference for inequality.  (Contributed by NM, 29-Apr-2005.) */

theorem neeq1i (A : class, B : class, C : class)  {
	hyp 1 : wff = |- A = B ;;
	-----------------------
	prop 1 : wff = |- ( A ≠ C ↔ B ≠ C ) ;;
}

proof of neeq1i {
	step 1 : wff = neeq1 () |- ( A = B → ( A ≠ C ↔ B ≠ C ) ) ;;
	step 2 : wff = ax-mp (hyp 1, step 1) |- ( A ≠ C ↔ B ≠ C ) ;;
	qed prop 1 = step 2 ;;
}

