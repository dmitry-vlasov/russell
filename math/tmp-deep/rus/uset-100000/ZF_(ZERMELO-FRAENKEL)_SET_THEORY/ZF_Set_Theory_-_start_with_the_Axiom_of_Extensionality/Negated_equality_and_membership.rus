proof of pm13.181 {
	step 1 : wff = eqcom () |- ( A = B ↔ B = A ) ;;
	step 2 : wff = pm13.18 () |- ( ( B = A ∧ B ≠ C ) → A ≠ C ) ;;
	step 3 : wff = sylanb (step 1, step 2) |- ( ( A = B ∧ B ≠ C ) → A ≠ C ) ;;
	qed prop 1 = step 3 ;;
}

/*A contradiction implies anything.  Equality/inequality deduction form.
       (Contributed by David Moews, 28-Feb-2017.) */

theorem pm2.21ddne (ph : wff, ps : wff, A : class, B : class)  {
	hyp 1 : wff = |- ( ph → A = B ) ;;
	hyp 2 : wff = |- ( ph → A ≠ B ) ;;
