	step 3 : wff = anbi12i (step 1, step 2) |- ( ( A ≠ B ∧ C ≠ D ) ↔ ( ¬ A = B ∧ ¬ C = D ) ) ;;
	step 4 : wff = pm4.56 () |- ( ( ¬ A = B ∧ ¬ C = D ) ↔ ¬ ( A = B ∨ C = D ) ) ;;
