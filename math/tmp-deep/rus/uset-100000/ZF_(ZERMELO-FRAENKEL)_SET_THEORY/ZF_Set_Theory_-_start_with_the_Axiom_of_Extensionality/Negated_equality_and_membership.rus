/*An inference from an inequality, related to modus tollens.  (Contributed
       by NM, 13-Apr-2007.) */

theorem nemtbir (ph : wff, A : class, B : class)  {
	hyp 1 : wff = |- A ≠ B ;;
	hyp 2 : wff = |- ( ph ↔ A = B ) ;;
	-----------------------
	prop 1 : wff = |- ¬ ph ;;
}

proof of nemtbir {
	step 1 : wff = df-ne () |- ( A ≠ B ↔ ¬ A = B ) ;;
	step 2 : wff = mpbi (hyp 1, step 1) |- ¬ A = B ;;
	step 3 : wff = mtbir (step 2, hyp 2) |- ¬ ph ;;
	qed prop 1 = step 3 ;;
}

/*Two classes are different if they don't contain the same element.
     (Contributed by NM, 3-Feb-2012.) */

theorem nelne1 (A : class, B : class, C : class)  {
	prop 1 : wff = |- ( ( A ∈ B ∧ ¬ A ∈ C ) → B ≠ C ) ;;
}

proof of nelne1 {
	step 1 : wff = eleq2 () |- ( B = C → ( A ∈ B ↔ A ∈ C ) ) ;;
	step 2 : wff = biimpcd (step 1) |- ( A ∈ B → ( B = C → A ∈ C ) ) ;;
	step 3 : wff = necon3bd (step 2) |- ( A ∈ B → ( ¬ A ∈ C → B ≠ C ) ) ;;
	step 4 : wff = imp (step 3) |- ( ( A ∈ B ∧ ¬ A ∈ C ) → B ≠ C ) ;;
	qed prop 1 = step 4 ;;
}

/*Two classes are different if they don't belong to the same class.
