	step 4 : wff = imp (step 3) |- ( ( A ∈ C ∧ ¬ B ∈ C ) → A ≠ B ) ;;
	qed prop 1 = step 4 ;;
}

/*Equality theorem for negated membership.  (Contributed by NM,
