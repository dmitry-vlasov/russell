	step 6 : wff = rexbii (step 5) |- ( âˆƒ z âˆˆ { x âˆˆ A | ph } âˆ€ y âˆˆ { x âˆˆ A | ph } Â¬ y R z â†” âˆƒ z âˆˆ { x âˆˆ A | ph } âˆ€ y âˆˆ A ( ps â†’ Â¬ y R z ) ) ;;
	step 7 : wff = breq2 () |- ( z = x â†’ ( y R z â†” y R x ) ) ;;
	step 8 : wff = notbid (step 7) |- ( z = x â†’ ( Â¬ y R z â†” Â¬ y R x ) ) ;;
	step 9 : wff = imbi2d (step 8) |- ( z = x â†’ ( ( ps â†’ Â¬ y R z ) â†” ( ps â†’ Â¬ y R x ) ) ) ;;
	step 10 : wff = ralbidv (step 9) |- ( z = x â†’ ( âˆ€ y âˆˆ A ( ps â†’ Â¬ y R z ) â†” âˆ€ y âˆˆ A ( ps â†’ Â¬ y R x ) ) ) ;;
	step 11 : wff = rexrab2 (step 10) |- ( âˆƒ z âˆˆ { x âˆˆ A | ph } âˆ€ y âˆˆ A ( ps â†’ Â¬ y R z ) â†” âˆƒ x âˆˆ A ( ph âˆ§ âˆ€ y âˆˆ A ( ps â†’ Â¬ y R x ) ) ) ;;
	step 12 : wff = bitri (step 6, step 11) |- ( âˆƒ z âˆˆ { x âˆˆ A | ph } âˆ€ y âˆˆ { x âˆˆ A | ph } Â¬ y R z â†” âˆƒ x âˆˆ A ( ph âˆ§ âˆ€ y âˆˆ A ( ps â†’ Â¬ y R x ) ) ) ;;
	step 13 : wff = sylib (step 4, step 12) |- ( ( ( { x âˆˆ A | ph } âˆˆ _V âˆ§ R Fr A ) âˆ§ ( { x âˆˆ A | ph } âŠ† A âˆ§ { x âˆˆ A | ph } â‰  âˆ… ) ) â†’ âˆƒ x âˆˆ A ( ph âˆ§ âˆ€ y âˆˆ A ( ps â†’ Â¬ y R x ) ) ) ;;
