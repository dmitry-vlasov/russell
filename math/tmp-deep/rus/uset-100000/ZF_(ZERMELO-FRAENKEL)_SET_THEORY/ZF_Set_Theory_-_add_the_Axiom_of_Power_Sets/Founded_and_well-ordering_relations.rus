	step 12 : wff = nfral (step 7, step 11) |- F/ x âˆ€ c âˆˆ a Â¬ c R b ;;
	step 13 : wff = nfrex (step 6, step 12) |- F/ x âˆƒ b âˆˆ a âˆ€ c âˆˆ a Â¬ c R b ;;
	step 14 : wff = nfim (step 5, step 13) |- F/ x ( ( a âŠ† A âˆ§ a â‰  âˆ… ) â†’ âˆƒ b âˆˆ a âˆ€ c âˆˆ a Â¬ c R b ) ;;
	step 15 : wff = nfal (step 14) |- F/ x âˆ€ a ( ( a âŠ† A âˆ§ a â‰  âˆ… ) â†’ âˆƒ b âˆˆ a âˆ€ c âˆˆ a Â¬ c R b ) ;;
	step 16 : wff = nfxfr (step 1, step 15) |- F/ x R Fr A ;;
