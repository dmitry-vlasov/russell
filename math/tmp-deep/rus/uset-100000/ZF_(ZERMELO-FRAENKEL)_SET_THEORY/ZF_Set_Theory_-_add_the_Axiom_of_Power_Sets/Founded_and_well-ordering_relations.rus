import uset-100000/ZF_(ZERMELO-FRAENKEL)_SET_THEORY/ZF_Set_Theory_-_add_the_Axiom_of_Power_Sets/Partial_and_complete_ordering.rus;;

/*=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
                Founded and well-ordering relations

=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
*/

/*Declare new constant symbols. */

constant {
	symbol Fr ;;
}

/*Well-founded predicate symbol (read: 'well-founded'). */

constant {
	symbol Se ;;
}

/*Set-like predicate symbol (read: 'set-like'). */

constant {
	symbol We ;;
}

/*Well-ordering predicate symbol (read: 'well-orders') */

/*Extend wff notation to include the well-founded predicate.  Read:  ' ` R `
     is a well-founded relation on ` A ` .' */

rule wfr (A : class, R : class) {
	term : wff = # R Fr A ;;
}

/*Extend wff notation to include the set-like predicate.  Read:  ' ` R ` is
     set-like on ` A ` .' */

rule wse (A : class, R : class) {
	term : wff = # R Se A ;;
}

/*Extend wff notation to include the well-ordering predicate.
     Read:  ' ` R ` well-orders ` A ` .' */

rule wwe (A : class, R : class) {
	term : wff = # R We A ;;
}

/*Define the well-founded relation predicate.  Definition 6.24(1) of
       [TakeutiZaring] p. 30.  For alternate definitions, see ~ dffr2 and
       ~ dffr3 .  (Contributed by NM, 3-Apr-1994.) */

definition df-fr (x : set, y : set, z : set, A : class, R : class) disjointed(x y z R, x y z A) {
	defiendum : wff = # R Fr A ;;
	definiens : wff = # âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ;;
	-----------------------
	prop : wff = |- ( defiendum â†” definiens ) ;;
}

/*Define the set-like predicate.  (Contributed by Mario Carneiro,
       19-Nov-2014.) */

definition df-se (x : set, y : set, A : class, R : class) disjointed(x y R, x y A) {
	defiendum : wff = # R Se A ;;
	definiens : wff = # âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ;;
	-----------------------
	prop : wff = |- ( defiendum â†” definiens ) ;;
}

/*Define the well-ordering predicate.  For an alternate definition, see
     ~ dfwe2 .  (Contributed by NM, 3-Apr-1994.) */

definition df-we (A : class, R : class)  {
	defiendum : wff = # R We A ;;
	definiens : wff = # ( R Fr A âˆ§ R Or A ) ;;
	-----------------------
	prop : wff = |- ( defiendum â†” definiens ) ;;
}

/*Property of well-founded relation (one direction of definition).
       (Contributed by NM, 18-Mar-1997.) */

theorem fri (x : set, y : set, A : class, B : class, C : class, R : class) disjointed(x y z A, x y z B, x y z R, x y) {
	prop 1 : wff = |- ( ( ( B âˆˆ C âˆ§ R Fr A ) âˆ§ ( B âŠ† A âˆ§ B â‰  âˆ… ) ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
}

proof of fri {
	var z : set;;
	step 1 : wff = df-fr () |- ( R Fr A â†” âˆ€ z ( ( z âŠ† A âˆ§ z â‰  âˆ… ) â†’ âˆƒ x âˆˆ z âˆ€ y âˆˆ z Â¬ y R x ) ) ;;
	step 2 : wff = sseq1 () |- ( z = B â†’ ( z âŠ† A â†” B âŠ† A ) ) ;;
	step 3 : wff = neeq1 () |- ( z = B â†’ ( z â‰  âˆ… â†” B â‰  âˆ… ) ) ;;
	step 4 : wff = anbi12d (step 2, step 3) |- ( z = B â†’ ( ( z âŠ† A âˆ§ z â‰  âˆ… ) â†” ( B âŠ† A âˆ§ B â‰  âˆ… ) ) ) ;;
	step 5 : wff = raleq () |- ( z = B â†’ ( âˆ€ y âˆˆ z Â¬ y R x â†” âˆ€ y âˆˆ B Â¬ y R x ) ) ;;
	step 6 : wff = rexeqbi1dv (step 5) |- ( z = B â†’ ( âˆƒ x âˆˆ z âˆ€ y âˆˆ z Â¬ y R x â†” âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ) ;;
	step 7 : wff = imbi12d (step 4, step 6) |- ( z = B â†’ ( ( ( z âŠ† A âˆ§ z â‰  âˆ… ) â†’ âˆƒ x âˆˆ z âˆ€ y âˆˆ z Â¬ y R x ) â†” ( ( B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ) ) ;;
	step 8 : wff = spcgv (step 7) |- ( B âˆˆ C â†’ ( âˆ€ z ( ( z âŠ† A âˆ§ z â‰  âˆ… ) â†’ âˆƒ x âˆˆ z âˆ€ y âˆˆ z Â¬ y R x ) â†’ ( ( B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ) ) ;;
	step 9 : wff = syl5bi (step 1, step 8) |- ( B âˆˆ C â†’ ( R Fr A â†’ ( ( B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ) ) ;;
	step 10 : wff = imp31 (step 9) |- ( ( ( B âˆˆ C âˆ§ R Fr A ) âˆ§ ( B âŠ† A âˆ§ B â‰  âˆ… ) ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
	qed prop 1 = step 10 ;;
}

/*The ` R ` -preimage of an element of the base set in a set-like relation
       is a set.  (Contributed by Mario Carneiro, 19-Nov-2014.) */

theorem seex (x : set, A : class, B : class, R : class) disjointed(x y A, x y B, x y R, x y) {
	prop 1 : wff = |- ( ( R Se A âˆ§ B âˆˆ A ) â†’ { x âˆˆ A | x R B } âˆˆ _V ) ;;
}

proof of seex {
	var y : set;;
	step 1 : wff = df-se () |- ( R Se A â†” âˆ€ y âˆˆ A { x âˆˆ A | x R y } âˆˆ _V ) ;;
	step 2 : wff = breq2 () |- ( y = B â†’ ( x R y â†” x R B ) ) ;;
	step 3 : wff = rabbidv (step 2) |- ( y = B â†’ { x âˆˆ A | x R y } = { x âˆˆ A | x R B } ) ;;
	step 4 : wff = eleq1d (step 3) |- ( y = B â†’ ( { x âˆˆ A | x R y } âˆˆ _V â†” { x âˆˆ A | x R B } âˆˆ _V ) ) ;;
	step 5 : wff = rspccva (step 4) |- ( ( âˆ€ y âˆˆ A { x âˆˆ A | x R y } âˆˆ _V âˆ§ B âˆˆ A ) â†’ { x âˆˆ A | x R B } âˆˆ _V ) ;;
	step 6 : wff = sylanb (step 1, step 5) |- ( ( R Se A âˆ§ B âˆˆ A ) â†’ { x âˆˆ A | x R B } âˆˆ _V ) ;;
	qed prop 1 = step 6 ;;
}

/*Any relation on a set is set-like on it.  (Contributed by Mario
       Carneiro, 22-Jun-2015.) */

theorem exse (A : class, R : class, V : class) disjointed(x y A, x y, x y R, x y V) {
	prop 1 : wff = |- ( A âˆˆ V â†’ R Se A ) ;;
}

proof of exse {
	var x : set, y : set;;
	step 1 : wff = rabexg () |- ( A âˆˆ V â†’ { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 2 : wff = ralrimivw (step 1) |- ( A âˆˆ V â†’ âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 3 : wff = df-se () |- ( R Se A â†” âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 4 : wff = sylibr (step 2, step 3) |- ( A âˆˆ V â†’ R Se A ) ;;
	qed prop 1 = step 4 ;;
}

/*Alternate definition of well-founded relation.  Similar to Definition
       6.21 of [TakeutiZaring] p. 30.  (Contributed by NM, 17-Feb-2004.)
       (Proof shortened by Andrew Salmon, 27-Aug-2011.)  (Proof shortened by
       Mario Carneiro, 23-Jun-2015.) */

theorem dffr2 (x : set, y : set, z : set, A : class, R : class) disjointed(x y z A, x y z R) {
	prop 1 : wff = |- ( R Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x { z âˆˆ x | z R y } = âˆ… ) ) ;;
}

proof of dffr2 {
	step 1 : wff = df-fr () |- ( R Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 2 : wff = rabeq0 () |- ( { z âˆˆ x | z R y } = âˆ… â†” âˆ€ z âˆˆ x Â¬ z R y ) ;;
	step 3 : wff = rexbii (step 2) |- ( âˆƒ y âˆˆ x { z âˆˆ x | z R y } = âˆ… â†” âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ;;
	step 4 : wff = imbi2i (step 3) |- ( ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x { z âˆˆ x | z R y } = âˆ… ) â†” ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 5 : wff = albii (step 4) |- ( âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x { z âˆˆ x | z R y } = âˆ… ) â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 6 : wff = bitr4i (step 1, step 5) |- ( R Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x { z âˆˆ x | z R y } = âˆ… ) ) ;;
	qed prop 1 = step 6 ;;
}

/*Property of well-founded relation (one direction of definition using
       class variables).  (Contributed by NM, 17-Feb-2004.)  (Revised by Mario
       Carneiro, 19-Nov-2014.) */

theorem frc (x : set, y : set, A : class, B : class, R : class) disjointed(x y A, x y B, x y R) {
	hyp 1 : wff = |- B âˆˆ _V ;;
	-----------------------
	prop 1 : wff = |- ( ( R Fr A âˆ§ B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B { y âˆˆ B | y R x } = âˆ… ) ;;
}

proof of frc {
	step 1 : wff = fri () |- ( ( ( B âˆˆ _V âˆ§ R Fr A ) âˆ§ ( B âŠ† A âˆ§ B â‰  âˆ… ) ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
	step 2 : wff = mpanl1 (hyp 1, step 1) |- ( ( R Fr A âˆ§ ( B âŠ† A âˆ§ B â‰  âˆ… ) ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
	step 3 : wff = 3impb (step 2) |- ( ( R Fr A âˆ§ B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
	step 4 : wff = rabeq0 () |- ( { y âˆˆ B | y R x } = âˆ… â†” âˆ€ y âˆˆ B Â¬ y R x ) ;;
	step 5 : wff = rexbii (step 4) |- ( âˆƒ x âˆˆ B { y âˆˆ B | y R x } = âˆ… â†” âˆƒ x âˆˆ B âˆ€ y âˆˆ B Â¬ y R x ) ;;
	step 6 : wff = sylibr (step 3, step 5) |- ( ( R Fr A âˆ§ B âŠ† A âˆ§ B â‰  âˆ… ) â†’ âˆƒ x âˆˆ B { y âˆˆ B | y R x } = âˆ… ) ;;
	qed prop 1 = step 6 ;;
}

/*Subset theorem for the well-founded predicate.  Exercise 1 of
       [TakeutiZaring] p. 31.  (Contributed by NM, 3-Apr-1994.)  (Proof
       shortened by Andrew Salmon, 25-Jul-2011.) */

theorem frss (A : class, B : class, R : class) disjointed(x y z A, x y z B, x y z R, x y) {
	prop 1 : wff = |- ( A âŠ† B â†’ ( R Fr B â†’ R Fr A ) ) ;;
}

proof of frss {
	var x : set, y : set, z : set;;
	step 1 : wff = sstr2 () |- ( x âŠ† A â†’ ( A âŠ† B â†’ x âŠ† B ) ) ;;
	step 2 : wff = com12 (step 1) |- ( A âŠ† B â†’ ( x âŠ† A â†’ x âŠ† B ) ) ;;
	step 3 : wff = anim1d (step 2) |- ( A âŠ† B â†’ ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ ( x âŠ† B âˆ§ x â‰  âˆ… ) ) ) ;;
	step 4 : wff = imim1d (step 3) |- ( A âŠ† B â†’ ( ( ( x âŠ† B âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) â†’ ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ) ;;
	step 5 : wff = alimdv (step 4) |- ( A âŠ† B â†’ ( âˆ€ x ( ( x âŠ† B âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) â†’ âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ) ;;
	step 6 : wff = df-fr () |- ( R Fr B â†” âˆ€ x ( ( x âŠ† B âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 7 : wff = df-fr () |- ( R Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 8 : wff = 3imtr4g (step 5, step 6, step 7) |- ( A âŠ† B â†’ ( R Fr B â†’ R Fr A ) ) ;;
	qed prop 1 = step 8 ;;
}

/*Subset theorem for the set-like predicate.  (Contributed by Mario
       Carneiro, 24-Jun-2015.) */

theorem sess1 (A : class, R : class, S : class) disjointed(x y A, x y, x y R, x y S) {
	prop 1 : wff = |- ( R âŠ† S â†’ ( S Se A â†’ R Se A ) ) ;;
}

proof of sess1 {
	var x : set, y : set;;
	step 1 : wff = simpl () |- ( ( R âŠ† S âˆ§ y âˆˆ A ) â†’ R âŠ† S ) ;;
	step 2 : wff = ssbrd (step 1) |- ( ( R âŠ† S âˆ§ y âˆˆ A ) â†’ ( y R x â†’ y S x ) ) ;;
	step 3 : wff = ss2rabdv (step 2) |- ( R âŠ† S â†’ { y âˆˆ A | y R x } âŠ† { y âˆˆ A | y S x } ) ;;
	step 4 : wff = ssexg () |- ( ( { y âˆˆ A | y R x } âŠ† { y âˆˆ A | y S x } âˆ§ { y âˆˆ A | y S x } âˆˆ _V ) â†’ { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 5 : wff = ex (step 4) |- ( { y âˆˆ A | y R x } âŠ† { y âˆˆ A | y S x } â†’ ( { y âˆˆ A | y S x } âˆˆ _V â†’ { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 6 : wff = syl (step 3, step 5) |- ( R âŠ† S â†’ ( { y âˆˆ A | y S x } âˆˆ _V â†’ { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 7 : wff = ralimdv (step 6) |- ( R âŠ† S â†’ ( âˆ€ x âˆˆ A { y âˆˆ A | y S x } âˆˆ _V â†’ âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 8 : wff = df-se () |- ( S Se A â†” âˆ€ x âˆˆ A { y âˆˆ A | y S x } âˆˆ _V ) ;;
	step 9 : wff = df-se () |- ( R Se A â†” âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 10 : wff = 3imtr4g (step 7, step 8, step 9) |- ( R âŠ† S â†’ ( S Se A â†’ R Se A ) ) ;;
	qed prop 1 = step 10 ;;
}

/*Subset theorem for the set-like predicate.  (Contributed by Mario
       Carneiro, 24-Jun-2015.) */

theorem sess2 (A : class, B : class, R : class) disjointed(x y A, x y B, x y R, x y) {
	prop 1 : wff = |- ( A âŠ† B â†’ ( R Se B â†’ R Se A ) ) ;;
}

proof of sess2 {
	var x : set, y : set;;
	step 1 : wff = ssralv () |- ( A âŠ† B â†’ ( âˆ€ x âˆˆ B { y âˆˆ B | y R x } âˆˆ _V â†’ âˆ€ x âˆˆ A { y âˆˆ B | y R x } âˆˆ _V ) ) ;;
	step 2 : wff = rabss2 () |- ( A âŠ† B â†’ { y âˆˆ A | y R x } âŠ† { y âˆˆ B | y R x } ) ;;
	step 3 : wff = ssexg () |- ( ( { y âˆˆ A | y R x } âŠ† { y âˆˆ B | y R x } âˆ§ { y âˆˆ B | y R x } âˆˆ _V ) â†’ { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 4 : wff = ex (step 3) |- ( { y âˆˆ A | y R x } âŠ† { y âˆˆ B | y R x } â†’ ( { y âˆˆ B | y R x } âˆˆ _V â†’ { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 5 : wff = syl (step 2, step 4) |- ( A âŠ† B â†’ ( { y âˆˆ B | y R x } âˆˆ _V â†’ { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 6 : wff = ralimdv (step 5) |- ( A âŠ† B â†’ ( âˆ€ x âˆˆ A { y âˆˆ B | y R x } âˆˆ _V â†’ âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 7 : wff = syld (step 1, step 6) |- ( A âŠ† B â†’ ( âˆ€ x âˆˆ B { y âˆˆ B | y R x } âˆˆ _V â†’ âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ) ;;
	step 8 : wff = df-se () |- ( R Se B â†” âˆ€ x âˆˆ B { y âˆˆ B | y R x } âˆˆ _V ) ;;
	step 9 : wff = df-se () |- ( R Se A â†” âˆ€ x âˆˆ A { y âˆˆ A | y R x } âˆˆ _V ) ;;
	step 10 : wff = 3imtr4g (step 7, step 8, step 9) |- ( A âŠ† B â†’ ( R Se B â†’ R Se A ) ) ;;
	qed prop 1 = step 10 ;;
}

/*Equality theorem for the well-founded predicate.  (Contributed by NM,
       9-Mar-1997.) */

theorem freq1 (A : class, R : class, S : class) disjointed(x y z R, x y z S, x y z A) {
	prop 1 : wff = |- ( R = S â†’ ( R Fr A â†” S Fr A ) ) ;;
}

proof of freq1 {
	var x : set, y : set, z : set;;
	step 1 : wff = breq () |- ( R = S â†’ ( z R y â†” z S y ) ) ;;
	step 2 : wff = notbid (step 1) |- ( R = S â†’ ( Â¬ z R y â†” Â¬ z S y ) ) ;;
	step 3 : wff = rexralbidv (step 2) |- ( R = S â†’ ( âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y â†” âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z S y ) ) ;;
	step 4 : wff = imbi2d (step 3) |- ( R = S â†’ ( ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) â†” ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z S y ) ) ) ;;
	step 5 : wff = albidv (step 4) |- ( R = S â†’ ( âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z S y ) ) ) ;;
	step 6 : wff = df-fr () |- ( R Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z R y ) ) ;;
	step 7 : wff = df-fr () |- ( S Fr A â†” âˆ€ x ( ( x âŠ† A âˆ§ x â‰  âˆ… ) â†’ âˆƒ y âˆˆ x âˆ€ z âˆˆ x Â¬ z S y ) ) ;;
	step 8 : wff = 3bitr4g (step 5, step 6, step 7) |- ( R = S â†’ ( R Fr A â†” S Fr A ) ) ;;
	qed prop 1 = step 8 ;;
}

/*Equality theorem for the well-founded predicate.  (Contributed by NM,
     3-Apr-1994.) */

theorem freq2 (A : class, B : class, R : class)  {
	prop 1 : wff = |- ( A = B â†’ ( R Fr A â†” R Fr B ) ) ;;
}

proof of freq2 {
	step 1 : wff = eqimss2 () |- ( A = B â†’ B âŠ† A ) ;;
	step 2 : wff = frss () |- ( B âŠ† A â†’ ( R Fr A â†’ R Fr B ) ) ;;
	step 3 : wff = syl (step 1, step 2) |- ( A = B â†’ ( R Fr A â†’ R Fr B ) ) ;;
	step 4 : wff = eqimss () |- ( A = B â†’ A âŠ† B ) ;;
	step 5 : wff = frss () |- ( A âŠ† B â†’ ( R Fr B â†’ R Fr A ) ) ;;
	step 6 : wff = syl (step 4, step 5) |- ( A = B â†’ ( R Fr B â†’ R Fr A ) ) ;;
	step 7 : wff = impbid (step 3, step 6) |- ( A = B â†’ ( R Fr A â†” R Fr B ) ) ;;
	qed prop 1 = step 7 ;;
}

/*Equality theorem for the set-like predicate.  (Contributed by Mario
     Carneiro, 24-Jun-2015.) */

theorem seeq1 (A : class, R : class, S : class)  {
