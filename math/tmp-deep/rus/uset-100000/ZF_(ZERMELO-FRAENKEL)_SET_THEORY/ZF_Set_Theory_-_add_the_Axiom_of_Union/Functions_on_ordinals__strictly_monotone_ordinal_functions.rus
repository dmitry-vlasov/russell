/*Conditions for which ` A ` is a strictly monotone ordinal function.
       (Contributed by Andrew Salmon, 15-Nov-2011.) */

theorem issmo (x : set, y : set, A : class, B : class) disjointed(x y A) {
	hyp 1 : wff = |- A : B âŸ¶ On ;;
	hyp 2 : wff = |- Ord B ;;
	hyp 3 : wff = |- ( ( x âˆˆ B âˆ§ y âˆˆ B ) â†’ ( x âˆˆ y â†’ ( A ` x ) âˆˆ ( A ` y ) ) ) ;;
	hyp 4 : wff = |- dom A = B ;;
	-----------------------
	prop 1 : wff = |- Smo A ;;
}

proof of issmo {
	step 1 : wff = feq2i (hyp 4) |- ( A : dom A âŸ¶ On â†” A : B âŸ¶ On ) ;;
	step 2 : wff = mpbir (hyp 1, step 1) |- A : dom A âŸ¶ On ;;
	step 3 : wff = ordeq () |- ( dom A = B â†’ ( Ord dom A â†” Ord B ) ) ;;
	step 4 : wff = ax-mp (hyp 4, step 3) |- ( Ord dom A â†” Ord B ) ;;
	step 5 : wff = mpbir (hyp 2, step 4) |- Ord dom A ;;
	step 6 : wff = eleq2i (hyp 4) |- ( x âˆˆ dom A â†” x âˆˆ B ) ;;
	step 7 : wff = eleq2i (hyp 4) |- ( y âˆˆ dom A â†” y âˆˆ B ) ;;
	step 8 : wff = syl2anb (step 6, step 7, hyp 3) |- ( ( x âˆˆ dom A âˆ§ y âˆˆ dom A ) â†’ ( x âˆˆ y â†’ ( A ` x ) âˆˆ ( A ` y ) ) ) ;;
	step 9 : wff = rgen2a (step 8) |- âˆ€ x âˆˆ dom A âˆ€ y âˆˆ dom A ( x âˆˆ y â†’ ( A ` x ) âˆˆ ( A ` y ) ) ;;
	step 10 : wff = df-smo () |- ( Smo A â†” ( A : dom A âŸ¶ On âˆ§ Ord dom A âˆ§ âˆ€ x âˆˆ dom A âˆ€ y âˆˆ dom A ( x âˆˆ y â†’ ( A ` x ) âˆˆ ( A ` y ) ) ) ) ;;
	step 11 : wff = mpbir3an (step 2, step 5, step 9, step 10) |- Smo A ;;
	qed prop 1 = step 11 ;;
}

