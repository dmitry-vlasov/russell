	var x : set, y : set;;
	step 1 : wff = id () |- ( x = C â†’ x = C ) ;;
	step 2 : wff = fveq2 () |- ( x = C â†’ ( F ` x ) = ( F ` C ) ) ;;
	step 3 : wff = sseq12d (step 1, step 2) |- ( x = C â†’ ( x âŠ† ( F ` x ) â†” C âŠ† ( F ` C ) ) ) ;;
