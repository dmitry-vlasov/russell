	step 12 : wff = dmeq () |- ( A = B â†’ dom A = dom B ) ;;
	step 13 : wff = raleqdv (step 12) |- ( A = B â†’ ( âˆ€ y âˆˆ dom A ( x âˆˆ y â†’ ( B ` x ) âˆˆ ( B ` y ) ) â†” âˆ€ y âˆˆ dom B ( x âˆˆ y â†’ ( B ` x ) âˆˆ ( B ` y ) ) ) ) ;;
	step 14 : wff = ralbidv (step 13) |- ( A = B â†’ ( âˆ€ x âˆˆ dom A âˆ€ y âˆˆ dom A ( x âˆˆ y â†’ ( B ` x ) âˆˆ ( B ` y ) ) â†” âˆ€ x âˆˆ dom A âˆ€ y âˆˆ dom B ( x âˆˆ y â†’ ( B ` x ) âˆˆ ( B ` y ) ) ) ) ;;
